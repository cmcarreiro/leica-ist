{
  "name": "quizzes-tutor-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve --fix",
    "build": "NODE_ENV=production vue-cli-service build --mode production",
    "build:dev": "NODE_ENV=development vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --single-quote --write \"**/**/*.{ts,js,css,scss,vue}\"",
    "codecov": "codecov",
    "cypress": "npx cypress open",
    "test:e2e": "npx cypress run",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "axios": "^0.21.1",
    "material-design-icons-iconfont": "^5.0.1",
    "qrcode": "^1.4.4",
    "sanitize-html": "^2.3.2",
    "showdown": "^1.9.1",
    "typeface-roboto": "0.0.75",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-codemirror": "^4.0.6",
    "vue-ctk-date-time-picker": "^2.5.0",
    "vue-property-decorator": "^8.5.1",
    "vue-qrcode": "^0.3.5",
    "vue-qrcode-reader": "^2.3.14",
    "vue-router": "^3.5.1",
    "vue-underscore": "^0.1.4",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.6.10",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@mdi/font": "^4.8.95",
    "@types/codemirror": "^0.0.102",
    "@types/jest": "^24.0.19",
    "@types/qrcode": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-plugin-pwa": "^4.5.6",
    "@vue/cli-plugin-router": "^4.5.6",
    "@vue/cli-plugin-typescript": "^4.5.6",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/cli-plugin-vuex": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.1.0",
    "@vue/test-utils": "^1.1.0",
    "axios-mock-adapter": "^1.17.0",
    "codecov": "^3.7.0",
    "compression-webpack-plugin": "^5.0.2",
    "cypress": "^6.9.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "glob-all": "^3.2.1",
    "prettier": "^1.19.1",
    "purgecss-webpack-plugin": "^1.6.0",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.0",
    "terser-webpack-plugin": "^2.3.8",
    "typescript": "~3.7.5",
    "vue-cli-plugin-vuetify": "^2.0.6",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "rules": {
      "no-console": 0,
      "error": "off",
      "no-debugger": 0,
      "quotes": [
        "error",
        "single"
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "transformIgnorePatterns": [
      "node_modules/(?!(vue-underscore)/)"
    ]
  },
  "engines": {
    "node": ">=12.14.0",
    "npm": ">=6.13.0"
  },
  "license": "MIT"
}
